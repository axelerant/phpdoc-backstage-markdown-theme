<article
        class="
            phpdocumentor-element
            -enum-case
        "
>
    <h4 class="phpdocumentor-element__name" id="enumcase_{{ case.name }}">
        {{ case.name }}
        {{ include('components/headerlink.html.twig', {'on': case}, with_context = false) }}
    </h4>
    {{ include('components/element-found-in.md.twig', {'node': case}) }}
    {{ include('components/summary.html.twig', {'node': case}) }}
    {{ include('components/deprecation.md.twig', {'node': case}) }}
    {{ include('components/description.md.twig', {'node': case}) }}
    {{ include('components/description.md.twig', {'node': case.var[0]}) }}
    {{ include ('components/tags.html.twig', {'node': case}) }}
    {{ include('components/attributes.md.twig' , {'node': case}) }}
</article>
